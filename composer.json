{
    "name": "lucasgiovanny/laravel-serpro-datavalid",
    "description": "Laravel package to use Serpro Datavalid API",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Giovanny",
            "email": "lucasgiovanny@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/support": "^7.16"
    },
    "require-dev": {
        "orchestra/testbench": "^5.2",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "lucasgiovanny\\SerproDataValid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "lucasgiovanny\\SerproDataValid\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "extra": {
        "laravel": {
            "providers": [
                "lucasgiovanny\\SerproDataValid\\SerproDataValidServiceProvider"
            ]
        }
    }
}